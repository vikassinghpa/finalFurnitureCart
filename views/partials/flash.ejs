<%if(success && success.length){%>
  <div class="position-flash1 alert alert-success alert-dismissible fade show" role="alert">
      <strong><%=success%></strong>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>

<%if(error && error.length){%>
  <div class="position-item1 alert alert-warning alert-dismissible fade show" role="alert">
      <strong><%=error%></strong>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>